{
  "name": "DiaryEntry",
  "type": "object",
  "properties": {
    "user_id": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "mood": {
      "type": "string",
      "enum": [
        "happy",
        "neutral",
        "sad",
        "anxious",
        "angry"
      ]
    },
    "sentiment_score": {
      "type": "number"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "entry_date": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "user_id",
    "content",
    "entry_date"
  ]
}
